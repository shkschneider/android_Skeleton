<resources>

    <!-- Theme colors (Material Design) -->
    <color name="primaryColor">@color/blueDark</color> <!-- darker tone -->
    <color name="secondaryColor">@color/blueLight</color> <!-- lighter tone -->
    <!-- Your theme is pretty much set now: the rest is optional tweaking -->
    <color name="statusBarColor">@color/secondaryColor</color> <!-- secondary or black -->
    <color name="navigationBarColor">@color/black</color> <!-- secondary or black -->
    <!-- You should not have to edit anything below this line, but you can -->
    <color name="actionBarColor">@color/primaryColor</color>
    <color name="actionBarForegroundColor">@color/white</color>
    <color name="contentBackgroundColor">@color/white</color>
    <color name="textPrimaryColor">@color/black</color>
    <color name="textSecondaryColor">@color/grey</color>
