<resources
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedResources">

    <!-- Theme colors (Material Design) -->

    <color tools:ignore="ResourceName" name="accentColor">@color/sk_material_amber</color>
    <color tools:ignore="ResourceName" name="primaryColor">@color/sk_material_orange500</color>
    <color tools:ignore="ResourceName" name="primaryColorDark">@color/sk_material_orange700</color>
    <!-- Your theme is pretty much set now: the rest is optional tweaking -->
    <color tools:ignore="ResourceName" name="actionBarColor">@color/primaryColor</color>
    <color tools:ignore="ResourceName" name="actionBarForegroundColor">@color/sk_android_white</color>
    <color tools:ignore="ResourceName" name="contentBackgroundColor">@color/sk_android_background</color>
    <color tools:ignore="ResourceName" name="cellBackgroundColor">@color/sk_android_white</color>
    <color tools:ignore="ResourceName" name="navigationDrawerBackgroundColor">@color/sk_android_white</color>
    <color tools:ignore="ResourceName" name="navigationDrawerForegroundColor">@color/sk_navigation_drawer</color>
    <color tools:ignore="ResourceName" name="textPrimaryColor">@color/sk_android_black</color>
    <color tools:ignore="ResourceName" name="textSecondaryColor">@color/sk_android_grey</color>
    <color tools:ignore="ResourceName" name="snackBarBackgroundColor">#ff323232</color>
    <color tools:ignore="ResourceName" name="snackBarForegroundColor">@color/sk_android_white</color>
    <color tools:ignore="ResourceName" name="controlOff">#727272</color>
    <color tools:ignore="ResourceName" name="controlOn">@color/accentColor</color>
    <color tools:ignore="ResourceName" name="_divider">#1f000000</color>
    <color tools:ignore="ResourceName" name="highlight">#fffbcc</color>

    <color tools:ignore="ResourceName" name="statusBarColor">@color/primaryColorDark</color> <!-- primaryColorDark or black -->
    <color tools:ignore="ResourceName" name="navigationBarColor">@color/sk_android_black</color> <!-- usually black -->

    <!-- Fix for preference-v17 -->
    <color tools:ignore="ResourceName" name="preference_accent">@color/accentColor</color>
    <color tools:ignore="ResourceName,PrivateResource" name="preference_fallback_accent_color">@color/accentColor</color>

    <!-- Fingerprint -->
    <color name="sk_fingerprint_warning">#f4511e</color>
    <color name="sk_fingerprint_hint">#42000000</color>
    <color name="sk_fingerprint_success">@color/accentColor</color>
    <color name="sk_fingerprint_error">#F4511E</color>
    <color name="sk_fingerprint">#FFFFFF</color>

</resources>
